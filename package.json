{
  "name": "@yuigoto/infiniship-js",
  "version": "1.0.0",
  "description": "Infiniship is a pixelated spaceship sprite generator.",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuigoto/infiniship-js.git"
  },
  "bugs": {
    "url": "https://github.com/yuigoto/infiniship-js/issues"
  },
  "homepage": "https://github.com/yuigoto/infiniship-js/tree/main#readme",
  "keywords": [
    "javascript",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "author": "Fabio Y. Goto <lab@yuiti.dev>",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
